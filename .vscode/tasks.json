{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet build",
			"type": "shell",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": "$msCompile"
		},
		{
			"type": "func",
			"dependsOn": "build",
			"options": {
				"cwd": "${workspaceFolder}/bin/Debug/netcoreapp2.1"
			},
			"command": "host start",
			"isBackground": true,
			"problemMatcher": "$func-watch"
		}
	]
}
